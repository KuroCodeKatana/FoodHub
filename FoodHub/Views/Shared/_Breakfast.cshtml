@model FoodHub.ViewModels.ItemEntry

<!-- Page Section Start -->

        <div class="col-md-12">
            <div class="row">
                @if (Model.BREAKFASTLIST != null)
                {
                    foreach (var Q in Model.BREAKFASTLIST)
                    {
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <div class="item">
                                <div class="img">
                                    <img src="@Q.IMG_FILE" id="img" width="110px" height="110px" style="max-height:100%; max-width:100%; vertical-align:middle;cursor:pointer" onclick="imgpreview(this.id);">

                                    @*<img src="assets/img/recipes/img-01.jpg" alt="">*@
                                </div>
                                <div class="details">
                                    <h3><a href="single_Item.html">@Q.ITEM_NM</a></h3>
                                    <p>@Q.ITEM_DESC</p>
                                    <a href="#" class="add-cart" onclick="AddToCart(@Q.ITEM_CD)">Add To Cart <i class="icon-cart"></i></a>
                                </div>
                                <div class="price">
                                    <span>Rs. @Q.PRICE</span>
                                </div>
                            </div>
                        </div>
                    }
                }



            </div>
        </div>
  

<!-- Page Section End -->